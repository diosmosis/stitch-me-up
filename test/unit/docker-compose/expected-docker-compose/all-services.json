{
  "networks": {
    "testMicrosvc_net": {},
    "feature-flags_net": {},
    "service-registry_net": {},
    "widget-service_net": {}
  },
  "services": {
    "testMicrosvc_redis": {
      "image": "redis",
      "networks": [
        "testMicrosvc_net"
      ]
    },
    "testmicrosvc": {
      "command": [
        "npm",
        "start"
      ],
      "depends_on": [
        "testMicrosvc_redis"
      ],
      "image": "node:4",
      "links": [
        "testMicrosvc_redis:redis"
      ],
      "networks": [
        "testMicrosvc_net",
        "default"
      ],
      "ports": [
        "3000:3000"
      ],
      "volumes": [
        "test/resources/working-cache-dir/testMicrosvc:/app"
      ],
      "working_dir": "/app"
    },
    "feature-flags_dynamo": {
      "image": "dynamo",
      "networks": [
        "feature-flags_net"
      ]
    },
    "feature-flags": {
      "command": [
        "npm",
        "start"
      ],
      "depends_on": [
        "feature-flags_dynamo"
      ],
      "image": "node:4",
      "links": [
        "feature-flags_dynamo:dynamo"
      ],
      "networks": [
        "feature-flags_net",
        "default"
      ],
      "ports": [
        "3001:3000"
      ],
      "volumes": [
        "test/resources/working-cache-dir/feature-flags:/app"
      ],
      "working_dir": "/app"
    },
    "service-registry": {
      "command": [
        "npm",
        "start"
      ],
      "image": "node:4",
      "networks": [
        "service-registry_net",
        "default"
      ],
      "ports": [
        "3002:3000"
      ],
      "volumes": [
        "test/resources/working-cache-dir/service-registry:/app"
      ],
      "working_dir": "/app"
    },
    "widget-service_mysql": {
      "image": "mysql",
      "networks": [
        "widget-service_net"
      ]
    },
    "widget-service": {
      "command": [
        "npm",
        "start"
      ],
      "depends_on": [
        "widget-service_mysql"
      ],
      "image": "node:4",
      "links": [
        "widget-service_mysql:mysql"
      ],
      "networks": [
        "widget-service_net",
        "default"
      ],
      "ports": [
        "3003:3000"
      ],
      "volumes": [
        "test/resources/working-cache-dir/widget-service:/app"
      ],
      "working_dir": "/app"
    }
  },
  "version": "2"
}
